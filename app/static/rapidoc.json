{
  "openapi": "3.0.0",
  "info": {
    "title": "Bloxs-api",
    "description": "API de banco digital para teste técnico da bloxs",
    "version": "0.1"
  },
  "tags": [
    {
      "name": "Usuário",
      "description": "Rotas relacionadas ao usuário"
    },
