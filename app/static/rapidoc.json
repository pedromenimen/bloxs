              },
              "examples": {
                "0": {
                  "value": "{\r\n\t\"tipoConta\": \"002\",\r\n\t\"limiteSaqueDiario\": 1000\r\n}"
                },
                "1": {
                  "value": "{\r\n\t\"tipoConta\": \"2\",\r\n\t\"limiteSaqueDiario\": 1000\r\n}"
                },
                "2": {
                  "value": "{\r\n\t\"tipoConta\": \"002\",\r\n\t\"limiteSaqueDiario\": 1000\r\n}"
