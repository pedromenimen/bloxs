              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "idPessoa": {
                      "type": "integer"
                    },
                    "idConta": {
                      "type": "integer"
                    },
                    "limiteSaqueDiario": {
                      "type": "number"
